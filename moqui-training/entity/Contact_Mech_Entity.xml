<?xml version="1.0" encoding="UTF-8" ?>
<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/entity-definition-3.xsd">
    <entity entity-name="Contact_Mech" package="moqui.contact_mech">
        <field name="contact_mech_id" type="text-medium" size="20" is-pk="true"/>
        <field name="street_address" type="text-long" not-null="true"/>
        <field name="city" type="text-medium"/>
        <field name="state" type="text-medium"/>
        <field name="postal_code" type="text-medium"/>
        <field name="party_id" type="id" />
        <relationship type="one" related="moqui.party.Party" short-alias="party_id"/>
    </entity>
</entities>
