<service verb="create#MoquiTraining" type="entity-auto" noun="moqui_training">
    <in-parameter>
        <!-- Input Parameters -->
        <parameter name="trainingName" type="String" required="true"/>
        <parameter name="trainingDate" type="Date"  format="MM/DD/YYYY" required="true"/>
    </in-parameter>
    <out-parameter>
        <parameter name="trainingId" type="String"/>
    </out-parameter>
    <validations>
        <!-- Input Validations -->
        <validation field="trainingName" type="required"/>
        <validation field="trainingDate" type="date" format="MM/dd/yyyy"/>
    </validations>
</service>